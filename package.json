{
  "name": "mapeo-mobile-shell",
  "version": "1.0.0",
  "main": "./src/App.tsx",
  "scripts": {
    "start": "npm run build:translations && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "extract-messages": "formatjs extract 'src/**/*.{ts,tsx}' --format crowdin --out-file ./messages/en.json",
    "build:translations": "node ./scripts/compile.js "
  },
  "dependencies": {
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@shopify/react-native-skia": "^0.1.182",
    "d3": "^7.8.4",
    "expo": "~48.0.6",
    "expo-location": "~15.1.1",
    "expo-status-bar": "~1.4.4",
    "react": "^18.2.0",
    "react-intl": "^6.3.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-vector-icons": "^9.2.0",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@formatjs/cli": "^6.0.4",
    "@types/d3": "^7.4.0",
    "@types/react": "~18.0.14",
    "@types/react-native-vector-icons": "^6.4.13",
    "glob": "^9.3.0",
    "husky": "^8.0.0",
    "mkdirp": "^2.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.4"
  },
  "private": true
}
