{
  "name": "mapeo-mobile-shell",
  "version": "1.0.0",
  "main": "./src/App.tsx",
  "scripts": {
    "start": "npm run build:translations && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "extract-messages": "formatjs extract 'src/*.{ts,tsx}' --format crowdin --out-file ./messages/en.json",
    "build:translations": "node ./scripts/compile.js "
  },
  "dependencies": {
    "expo": "~48.0.6",
    "expo-status-bar": "~1.4.4",
    "react": "^18.2.0",
    "react-intl": "^6.3.0",
    "react-native": "0.71.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@formatjs/cli": "^6.0.4",
    "@types/react": "~18.0.14",
    "glob": "^9.3.0",
    "husky": "^8.0.0",
    "mkdirp": "^2.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.4"
  },
  "private": true
}
